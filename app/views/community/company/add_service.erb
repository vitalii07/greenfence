<div class="view_container">
  <div class="add_info_main_header">
		<h1 class="content_headline" ng-if="!store.id"><%= t('add_a_service') %></h1>
		<h1 class="content_headline" ng-if="store.id"><%= t('view_or_edit_a_service') %></h1>
		<a class="wizard_delete_butt" ><%= t('delete') %></a>
		<a class="wizard_add_butt_v2" ng-click="createService(service)"><%= t('save') %></a>
  </div>
  <div id='service_img_crop' style="display: none">
		<div class="cropArea">
		  <img-crop image="myImage" result-image="myCroppedImage"></img-crop>
		</div>
		<div>
		  <div class="butt_save" ng-click="upload_service_image()" style="margin: 0">Crop</div>
		</div>
  </div>
  <div class="add_info_block title_mod">
		<div class="add_info_header">
		  <div class="add_info_title required_mod"><%= t('select_operation') %></div>
		  <div class="add_info_colapse_butt operation_box"></div>
		</div>
		<div class="add_info_content_block">
		  <div ng-if="store.operation_name" class="add_info_content"><p>{{store.operation_name}}</p></div>
		  <div class="add_info_form_block operation_box">
				<div class="add_info_form_w">
				  <dl class="form_cell form_cell_v1_mod">
						<dt class="form_c_hline form_v1_mod hline_hide_mod autocomplete_el_v1_mod">
						  <label for="Enter_name_of_category">Enter name of category</label>
						</dt>
						<dd class="form_c_f_w form_v1_mod hline_hide_mod autocomplete_el_v1_mod">
						  <ui-select ng-model="service.operation_id" theme="selectize">
								<ui-select-match placeholder="<%= t('select_an_operation_in_the_list_or_search_his_name') %>">{{$select.selected.name}}</ui-select-match>
								<ui-select-choices repeat="operation.id as operation in operations |  filter: $select.search">
								  <div ng-bind-html="operation.name | highlight: $select.search"></div>
								</ui-select-choices>
						  </ui-select>
						</dd>
				  </dl>
				</div>
		  </div>
		</div>
  </div>
  <div class="add_info_block textbox_mod" ng-show='service.operation_id || store.id'>
		<div class="add_info_header">
		  <div class="add_info_title required_mod"><%= t('enter_name_of_service') %></div>
		  <div class="add_info_colapse_butt"></div>
		</div>
		<div class="add_info_content_block">
		  <div class="add_info_content" ng-if="store.name">
				<p>{{store.name}}</p>
		  </div>
		  <div class="add_info_form_block">
				<div class="add_info_form_w">

				  <gf-text-input
				  id="'Enter_service_name'"
				  p-title="'<%= t('enter_name_of_service') %>'"
				  dl-class="" containers-class="" field-class="'default_mod'"
				  model="service.name">
				  </gf-text-input>

				  <div class="category_wrap">
						<ul class="category_list">
						  <li class="category_list_item">
								<label class="lbl_rb_ch_block lbl_rb_ch_default_mod">
								  <input name="Featured" type="checkbox" class="lbl_inp_rb_ch" ng-model="service.featured"><span class="lbl_rb_ch_text lbl_rb_ch_t_default_mod lbl_rb_ch_t_default_green_grey_mod ng-binding"><%= t('featured') %></span>
								</label>
						  </li>
						</ul>
				  </div>
				</div>
		  </div>
		</div>
  </div>
  <div class="add_info_block upload_mod" ng-show='service.operation_id || store.id'>
		<div class="add_info_header">
		  <div class="add_info_title"><%= t('upload_profile_picture') %></div>
		  <div class="add_info_colapse_butt"></div>
		</div>
		<div class="add_info_content_block">
		  <div class="add_info_content" ng-if="store.profile_picture.profile_picture.url && store.profile_picture.profile_picture.url != '/assets/avatar.jpg'">
				<a href="{{store.profile_picture.profile_picture.url}}" title="Click here to see file" class="file_link" download><%= t('download_file') %></a>
		  </div>
		  <div class="add_info_form_block">
				<div class="add_info_form_w">
				  <dl class="form_cell form_cell_v1_mod">
						<dt class="form_c_hline form_v1_mod hline_hide_mod">
						  <label for="Upload_image">Upload Image</label>
						</dt>
						<dd class="form_c_f_w form_v1_mod hline_hide_mod">
						  <input type="button" id="Upload_image" placeholder="<%= t('upload_image') %>" class="f_c_field default_mod file_mod" accept="image/*" ng-file-select ng-file-change="upload_image($files, $event)"/><span class="f_c_field_file_field"><span class="f_c_field_file_text">{{service.profile_picture.filename || "Upload image"}}</span><span class="f_c_field_file_butt">Choose File</span></span>
						</dd>
				  </dl>
				</div>
		  </div>
		</div>
  </div>
  <div class="add_info_block textarea_mod" ng-show='service.operation_id || store.id'>
		<div class="add_info_header">
		  <div class="add_info_title"><%= t('describe_your_service') %></div>
		  <div class="add_info_colapse_butt"></div>
		</div>
		<div class="add_info_content_block">
		  <div class="add_info_content" ng-if="store.description"><p>{{store.description}}</p></div>
		  <div class="add_info_form_block">
				<div class="add_info_form_w">
				  <dl class="form_cell form_cell_v1_mod">
						<dt class="form_c_hline form_v1_mod hline_hide_mod">
						  <label for="Enter_service_description">Description</label>
						</dt>
						<dd class="form_c_f_w form_v1_mod hline_hide_mod">
						  <textarea id="Enter_service_description" placeholder="<%= t('description') %>" class="f_c_field default_mod textarea_mod_1 " ng-model="service.description"></textarea>
						</dd>
				  </dl>
				</div>
		  </div>
		</div>
  </div>
  <div class="add_info_block upload_mod" ng-show='service.operation_id || store.id'>
