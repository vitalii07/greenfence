		  <div class="add_info_title"><%= t('upload_image') %></div>
		  <div class="add_info_colapse_butt"></div>
		</div>
		<div class="add_info_content_block">
		  <div class="add_info_content" ng-if="store.uploaded_files">
				<a ng-if="store.uploaded_files" ng-repeat="file in store.uploaded_files" href="{{file.image.url}}" title="Click here to see file" class="file_link ng-scope" download><%= t('download_file') %></a>
		  </div>
		  <div class="add_info_form_block">
				<div class="add_info_form_w">
				  <dl class="form_cell form_cell_v1_mod">
						<dt class="form_c_hline form_v1_mod hline_hide_mod">
						  <label for="Upload_image">Upload Image</label>
						</dt>
						<dd class="form_c_f_w form_v1_mod hline_hide_mod">
						  <input type="file" id="Upload_image" ng-model="service.uploaded_files_attributes[0].image" base-sixty-four-input placeholder="<%= t('upload_image') %>" class="f_c_field default_mod file_mod "/><span class="f_c_field_file_field"><span class="f_c_field_file_text">{{service.uploaded_files_attributes[0].image.filename || "Upload image"}}</span><span class="f_c_field_file_butt"><%= t('choose_file') %></span></span>
						</dd>
				  </dl>
				</div>
				<div class="butt_wrap add_info_form_mod">
				  <div class="attach_butt image_mod" ng-click="attach_multiple_images($event)"><%= t('attach_another_image') %></div>
				</div>
		  </div>
		</div>
